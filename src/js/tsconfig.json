{
  "compilerOptions": {
      "strictNullChecks": true,
      "noUnusedLocals" : true,
      "noImplicitThis": true,
      "alwaysStrict": true,
      "outDir": "./dist/",
      "declaration": true,
      "sourceMap": true,
      "target": "es5",
      "typeRoots": [
        "node_modules/@types"
      ],
      "types": [ "firebase" ],
      "module": "commonJS",
      "baseUrl": ".",
      "paths": {
          "*": ["./typings/*"]
      }
  },
    "exclude": [
      "node_modules"
    ]
}